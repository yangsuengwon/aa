<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ResultActivity">

    <TextView
        android:id="@+id/tv_MainName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MainName"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/tv_MainBirth"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MainBirth"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/Imgview_MainImg"
        android:layout_width="match_parent"
        android:layout_height="326dp"
        app:srcCompat="@drawable/stevencarsn_1399466648139149314_20210531_164344_img1" />

    <Button
        android:id="@+id/bt_GoMain4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Main" />

</LinearLayout>